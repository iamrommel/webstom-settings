<keymap version="1" name="Main Keyboard Map" parent="$default">
  <action id="CheckinProject">
    <keyboard-shortcut first-keystroke="shift ctrl k" />
  </action>
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl subtract" />
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
    <keyboard-shortcut first-keystroke="ctrl m" second-keystroke="ctrl l" />
  </action>
  <action id="EditorScrollToCenter" />
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <mouse-shortcut keystroke="control alt button1" />
    <keyboard-shortcut first-keystroke="f12" />
  </action>
  <action id="JumpToLastWindow" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl d" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl r" second-keystroke="ctrl r" />
  </action>
  <action id="Replace" />
  <action id="ScrollTreeToCenter" />
  <action id="Vcs.Push" />
  <action id="Vcs.ShowMessageHistory">
    <keyboard-shortcut first-keystroke="ctrl e" />
  </action>
</keymap>